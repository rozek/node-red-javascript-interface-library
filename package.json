{
  "name": "node-red-javascript-interface-library",
  "version": "1.0.1",
  "description": "auxiliary module to let Node-RED import the javascript-interface-library",
  "type": "module",
  "main": "./dist/node-red-javascript-interface-library.js",
  "exports": {
    ".": {
      "require": "./dist/node-red-javascript-interface-library.js",
      "import": "./dist/node-red-javascript-interface-library.esm.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "agadoo": "agadoo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rozek/node-red-javascript-interface-library.git"
  },
  "keywords": [
    "javascript",
    "node-red",
    "type-checking",
    "input-validation",
    "utility-functions"
  ],
  "author": "Andreas Rozek <a.rozek@gmx.de> (https://www.rozek.de/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rozek/node-red-javascript-interface-library/issues"
  },
  "homepage": "https://github.com/rozek/node-red-javascript-interface-library#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "agadoo": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "javascript-interface-library": "^1.0.5"
  }
}
